<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fx:id="erbScrollPane" fitToHeight="true" fitToWidth="true" prefHeight="750.0" prefWidth="1150.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox fx:id="erbVBox">
         <children>
            <MenuBar nodeOrientation="LEFT_TO_RIGHT">
              <menus>
                  <Menu fx:id="introMenu" mnemonicParsing="false" text="Intro Materials" />
                <Menu fx:id="resourcesMenu" mnemonicParsing="false" text="Resources" />
              </menus>
            </MenuBar>
            <HBox fx:id="breadCrumbHBox" />
            <VBox fx:id="welcomeVBox" alignment="CENTER">
               <children>
                  <Label fx:id="titleLabel" text="ERB">
                     <font>
                        <Font name="System Bold" size="30.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox fx:id="erbContainer" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </content>
</ScrollPane>
