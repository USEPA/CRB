<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="nodeVBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane VBox.vgrow="ALWAYS">
         <center>
            <HBox fx:id="hBox" alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox fx:id="formVBox" minWidth="400.0" spacing="5.0" HBox.hgrow="ALWAYS">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding></VBox>
                  <VBox fx:id="rightPanelVBox" minWidth="200.0" spacing="25.0" HBox.hgrow="ALWAYS">
                     <children>
                        <VBox fx:id="topPanelVBox" spacing="10.0" VBox.vgrow="ALWAYS">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding></VBox>
                        <Separator fx:id="separator" />
                        <VBox fx:id="bottomPanelVBox" alignment="BOTTOM_LEFT" spacing="10.0" VBox.vgrow="ALWAYS">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding></VBox>
                     </children>
                  </VBox>
               </children>
               <BorderPane.margin>
                  <Insets top="3.0" />
               </BorderPane.margin>
            </HBox>
         </center>
         <top>
            <Pane fx:id="tP" prefHeight="10.0" BorderPane.alignment="CENTER" />
         </top>
         <left>
            <Pane fx:id="lP" prefWidth="10.0" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Pane fx:id="rP" prefWidth="10.0" BorderPane.alignment="CENTER" />
         </right>
         <bottom>
            <Pane fx:id="bP" prefHeight="10.0" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
