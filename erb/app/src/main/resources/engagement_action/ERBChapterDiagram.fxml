<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.Polygon?>

<HBox fx:id="diagramHBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <HBox alignment="CENTER">
                     <children>
                        <Line fx:id="leftLeadingLine" endX="-50.0" startX="-100.0" />
                        <StackPane>
                           <children>
                              <Circle fx:id="centerCircle" fill="WHITE" radius="21.0" stroke="BLACK" strokeType="INSIDE" style="-fx-fill: #A9A9A9;" />
                              <Label fx:id="centerCircleLabel" />
                           </children>
                        </StackPane>
                        <Line fx:id="rightLeadingLine" endX="-50.0" startX="-100.0" />
                     </children>
                  </HBox>
                  <VBox fx:id="arrowVBox" alignment="CENTER_LEFT">
                     <children>
                        <Polygon points="28.0, -17.5, 56.0, -17.5, 41.5, -45.5" rotate="90.0" stroke="BLACK" strokeType="INSIDE" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <VBox alignment="TOP_CENTER" HBox.hgrow="ALWAYS">
                     <children>
                        <Label fx:id="chapterLabel" />
                     </children>
                  </VBox>
                  <VBox fx:id="arrowVBox1" alignment="CENTER_LEFT">
                     <children>
                        <Polygon points="28.0, -17.5, 56.0, -17.5, 41.5, -45.5" rotate="90.0" stroke="BLACK" strokeType="INSIDE" style="-fx-fill: transparent; -fx-stroke: transparent;" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
