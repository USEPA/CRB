<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="engagementVBox" prefHeight="750.0" prefWidth="1150.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="saveHBox" alignment="CENTER_RIGHT">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="20.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="goalLabel" text="Goal">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="goalComboBox" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonAction" text="Save" />
         </children>
      </HBox>
      <Pane maxHeight="2.0" minHeight="2.0" prefHeight="2.0" style="-fx-background-color: #CCCCCC;" />
      <Label fx:id="exploreModeLabel" text="Important: You are in Explore Mode. In order to use the interactive activites or to edit forms, create a project on the ERB Home page">
         <font>
            <Font name="System Italic" size="13.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button mnemonicParsing="false" onAction="#myPortfolioButtonAction" text="MyPortfolio">
               <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox fx:id="navigationHBox" alignment="TOP_CENTER">
         <children>
            <HBox fx:id="previousHBox" alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
            <HBox fx:id="nextHBox" alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets left="3.0" right="3.0" />
         </VBox.margin>
      </HBox>
      <HBox fx:id="bodyHBox" VBox.vgrow="ALWAYS">
         <children>
            <VBox fx:id="treeViewVBox">
               <children>
                  <HBox>
                     <children>
                        <HBox HBox.hgrow="ALWAYS">
                           <children>
                              <Button fx:id="previousButton" mnemonicParsing="false" onAction="#previousButtonAction" text="Previous" />
                           </children>
                        </HBox>
                        <HBox alignment="TOP_RIGHT" HBox.hgrow="ALWAYS">
                           <children>
                              <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextButtonAction" text="Next" />
                           </children>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <TreeView fx:id="treeView" minWidth="250.0" prefHeight="250.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox fx:id="mainVBox" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
               <children>
                  <HBox fx:id="body2HBox" VBox.vgrow="ALWAYS">
                     <children>
                        <VBox fx:id="contentVBox" alignment="CENTER" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
